@{
    ViewBag.Title = "ExpiredLink";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row h-p100 align-items-center justify-content-center">
    <div class="col-lg-8 col-11">
        <div style="margin-left:35%;">
            <img src="~/images/logo-light.png" alt="logo" class="light-logo">
        </div>
        <div class="error-page bg-white rounded">
            <div class="error-content">
                <div class="row h-p100 align-items-center bg-bubbles-dark bg-danger">
                    <div class="col-lg-12 col-md-12 bg-white text-dark">
                        <div class="p-30">
                            <h2 class="text-danger font-size-40 font-weight-700">Accès non autotisé</h2>
                            <h3>Lien expiré</h3>
                            <h5>Vous n'êtes pas autorisé</h5>
                            <p>Ce lien n'est plus accesible ou alors vous n'avez pas les droits necessaires. Si vous venez de le recevoir bien vouloir contactez le support à l'adresse <span class="text-primary">contact@africa-med.eu</span></p>
                            <div class="mb-15">
                                <a href="@Url.Action("login","admin")" class="btn btn-info btn-block margin-top-10">Retour à la page de login</a>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
            <!-- /.error-content -->
        </div>
    </div>
</div>



